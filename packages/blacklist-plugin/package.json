{
  "name": "@otterbots/blacklist-plugin",
  "version": "1.1.2",
  "description": "Blacklist plugin for the Sapphire Framework",
  "main": "dist/",
  "repository": "https://github.com/Otter-Bots/Subpackages.git",
  "author": "@Otter-Dev",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@sapphire/framework": "^3.0.0",
